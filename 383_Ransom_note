class Solution {
public:
    bool canConstruct(string ransomNote, string magazine) {
        for (int j = 0; j < ransomNote.size(); j++) {
            bool found = false;
            for (int i = 0; i < magazine.size(); i++) {
                if (ransomNote[j] == magazine[i]) {
                    magazine.erase(i, 1); 
                    found = true;
                    break;
                }
            }
            if (!found) return false; 
        }
        return true;
    }
};
